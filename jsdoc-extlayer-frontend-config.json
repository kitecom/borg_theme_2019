{
	"tags": {
		"allowUnknownTags": true
	},
	"source": {
		"include": [
			"Modules/suitecommerce"
		],
		"excludePattern": "^((?!APIdocs@).)*$",
		"includePattern": ".*(JavaScript|extlayercommon).*",

		"exclude": [
			"node_modules",
			"ns_npm_repository",
			"gulp"
		]
	},
	"plugins": ["plugins/markdown", "node_modules/jsdoc-typeof-plugin"],
	"templates": {
		"cleverLinks": false,
		"monospaceLinks": false,
		"default": {
			"outputSourceFiles": true
		},
		"copyright": "Copyright Â© 2005, 2017, Oracle and/or its affiliates. All rights reserved.",
		"systemName": "SuiteCommerce",
		"systemSummary": "SuiteCommerce Front-end Extension APIs",
		"systemLogo": "jsdoc-logo.png"
	},
	"opts": {
		"encoding": "utf8",
		"destination": "./jsdoc-extlayer-frontend/",
		"recurse": true,
		"tutorials": "Modules/suitecommerce/APIdocs@sc-2018.2.0/tutorials",
		"readme": "Modules/suitecommerce/APIdocs@sc-2018.2.0/tutorials/frontend_index.md"
	},
	"markdown.hardwrap": true,
	"markdown": {
		"hardwrap": true
	}
}


